{
  target {
    foreign {
      main "foreign.nt",
      build-option [
        "-fsanitize=address",
      ],
    },
  },
  foreign {
    input [
      "foreign/add_const.c",
    ],
    output [
      "add_const.o",
    ],
    script [
      "{{clang}} -c -flto=thin -O2 foreign/add_const.c -o {{foreign}}/add_const.o",
    ],
  },
  dependency {
    core {
      digest "Gqqa9FY2WS877MlG-wkGQvejwae4dipQ32MD6njVaIQ",
      mirror [
        "https://github.com/vekatze/neut-core/raw/main/archive/0-50-1.tar.zst",
      ],
      enable-preset true,
    },
  },
}
