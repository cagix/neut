variant nat {
- Zero
- Succ(nat)
}

define interpret(n: nat): int {
  match n {
  - Zero =>
    0
  - Succ(m) =>
    add-int(1, interpret(m))
  }
}

define add-nat(x: nat, y: nat): nat {
  match x {
  - Zero =>
    y
  - Succ(m) =>
    add-nat(m, Succ(y))
  }
}

define mul-nat(x: nat, y: nat): nat {
  match x {
  - Zero =>
    Zero
  - Succ(m) =>
    add-nat(mul-nat(m, y), y)
  }
}

define fold(a: tau, fs: list(a -> a), x: a): a {
  match fs {
  - Nil =>
    x
  - Cons(f, rest) =>
    let _ = rest
    let _ = rest
    f(fold(a, rest, x))
  }
}

define main(): int {
  let n1 = Succ(Zero)
  let n2 = Succ(n1)
  let n3 = Succ(n2)
  let add2 = lambda (x) { add-nat(x, n2) }
  let mul3 = lambda (x) { mul-nat(x, n3) }
  let mul9 = lambda (x) { mul-nat(x, mul-nat(n3, n3)) }
  let _ = mul9
  let _ = mul9

  let _ = lambda (x) { add-int(x, 2) }
  let _ = lambda (x) { mul-int(x, 3) }
  let _ = lambda (x) { sub-int(x, 1) }
  let fs = Cons(add2, Cons(mul3, Cons(mul9, Nil)))
  let gs = fs
  let hs = gs
  let _ = interpret(fold(_, hs, n3))
  0
}
