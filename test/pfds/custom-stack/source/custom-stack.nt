import {
  core.text.io {print-int},
}

data custom-stack(a) {
| N
| C(pair(a, a), custom-stack(a))
}

define empty(a: type): custom-stack(a) {
  N
}

define is-empty(a: type, xs: &custom-stack(a)): bool {
  case xs {
  | N =>
    True
  | C(_, _) =>
    False
  }
}

define head(a: type, xs: &custom-stack(a)): ?&pair(a, a) {
  case xs {
  | N =>
    none
  | C(a, _) =>
    Right(a)
  }
}

define tail(a: type, xs: &custom-stack(a)): ?&custom-stack(a) {
  case xs {
  | N =>
    none
  | C(_, rest) =>
    Right(rest)
  }
}

define main(): unit {
  let !xs = empty(() -> int) in
  let !v =
    Pair(function () {
      1
    }, function () {
      2
    })
  in
  let ys = C(!v, !xs) in
  let zs = C(!v, C(!v, ys)) in
  let calc on zs, !xs =
    let _ = is-empty(_, zs) in
    let _ = is-empty(_, !xs) in
    let h = head(_, zs) in
    let _ = head(_, !xs) in
    let _ = tail(_, zs) in
    let _ = tail(_, zs) in
    match h {
    | Left(_) =>
      1
    | Right(pair) =>
      tie Pair(_, right) = pair in
      {*right}()
    }
  in
  let _ = zs in
  let _ = !xs in
  print-int(calc);
  print("\n")
}
