import {
- core.bool
- core.top
- core.option
- core.list => L
- core.text => T
}

variant custom-stack(a) {
- Nil
- Cons(product(a, a), custom-stack(a))
}

define empty[a](): custom-stack(a) {
  Nil
}

define is-empty[a](xs: &custom-stack(a)): bool {
  case xs {
  - Nil =>
    True
  - Cons(y, rest) =>
    False
  }
}

define head[a](xs: &custom-stack(a)): ?&{a * a} {
  case xs {
  - Nil =>
    None
  - Cons(a, rest) =>
    Some(a)
  }
}

define tail[a](xs: &custom-stack(a)): ?&custom-stack(a) {
  case xs {
  - Nil =>
    None
  - Cons(a, rest) =>
    Some(rest)
  }
}

define main(): i64 {
  let xs = empty()
  let v = tuple(lambda () { 1 }, lambda () { 2: i64 })
  let ys = Cons(v, xs)
  let zs = Cons(v, Cons(v, ys))
  let calc on zs, xs = {
    let b = is-empty(zs)
    let b2 = is-empty(xs)
    let h = head(zs)
    let h2 = head(xs)
    let t = tail(zs)
    let t2 = tail(zs)
    match h {
    - None =>
      1
    - Some(pair) =>
      bind tuple(left, right) = pair
      {!right}()
    }
  }
  calc
}
