variant nat {
- Zero()
- Succ(nat)
}

define nil-cons-cons(xs: nat): int {
  match xs {
  - Zero() =>
    1
  - Succ(Succ(_)) =>
    2
  - Succ(_) =>
    3
  }
}

define main(): int {
  let n0 = Zero()
  let n1 = Succ(n0)
  let n2 = Succ(Succ(Zero()))
  let z0 = nil-cons-cons(n0)
  let z1 = nil-cons-cons(n1)
  let z2 = nil-cons-cons(n2)
  add-int(z0, add-int(z1, z2))
}
