import {
- core.bool
- core.list => L
- core.vector => V
}

variant list() {
- Nil()
- Cons(i64, list())
}

define length(xs: list()): i64 {
  match xs {
  - Nil() =>
    0
  - Cons(_, ys) =>
    add-i64(1, length(ys))
  }
}

define main(): i64 {
  let xs = Nil()
  let no-free-var =
    define foo(c: i64): i64 {
      if eq-i64(c, 0) {
        1
      } else {
        foo(sub-i64(c, 1))
      }
    }
  let _ = no-free-var(3)
  let single-free-var =
    define free-vars-in-recursion(c: i64): i64 {
      if eq-i64(c, 0) {
        let _ = xs
        let _ = length(xs)
        1
      } else {
        free-vars-in-recursion(sub-i64(c, 1))
      }
    }
  let _ = single-free-var(3)
  let ys = xs
  let zs = xs
  let ws = Nil()
  let multiple-free-var =
    define free-vars-in-recursion(c: i64): i64 {
      if eq-i64(c, 0) {
        let _ = xs
        let _ = xs
        let _ = ys
        let _ = length(ys)
        let _ = length(zs)
        let _ = zs
        1
      } else {
        let _ = xs
        let _ = ys
        let _ = ws
        free-vars-in-recursion(sub-i64(c, 1))
      }
    }
  multiple-free-var(3)
}
