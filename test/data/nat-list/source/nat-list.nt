variant nat {
- Zero()
- Succ(nat)
}

variant list(a: tau) {
- Nil()
- Cons(a, list(a))
}

define interpret(n: nat): int {
  match n {
  - Zero() =>
    0
  - Succ(m) =>
    add-int(1, interpret(m))
  }
}

define nat-list-sum(xs: list(nat)): int {
  match xs {
  - Nil() =>
    0
  - Cons(y, ys) =>
    add-int(interpret(y), nat-list-sum(ys))
  }
}


define main(): int {
  let _ = nat-list-sum(Cons(Succ(Zero()), Cons(Succ(Zero()), Nil())))
  0
}
