variant nat {
- Zero()
- Succ(nat)
}

variant list(a: tau) {
- Nil()
- Cons(a, list(a))
}

define interpret(n: nat): i64 {
  match n {
  - Zero() =>
    0
  - Succ(m) =>
    add-i64(1, interpret(m))
  }
}

define nat-list-sum(xs: list(nat)): i64 {
  match xs {
  - Nil() =>
    0
  - Cons(y, ys) =>
    add-i64(interpret(y), nat-list-sum(ys))
  }
}


define main(): i64 {
  nat-list-sum(Cons(Succ(Zero()), Cons(Succ(Zero()), Nil())))
}
