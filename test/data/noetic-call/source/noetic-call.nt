variant list(a: tau) {
- Nil()
- Cons(a, list(a))
}

define length(xs: &list(i64)): i64 {
  let a = xs
  let b = xs
  let c = xs
  case c {
  - Nil() =>
    0
  - Cons(y, ys) =>
    add-i64(1, length(ys))
  }
}

define tail(xs: &list(i64)): &list(i64) {
  case xs {
  - Nil() =>
    xs
  - Cons(y, ys) =>
    ys
  }
}

define main(): i64 {
  let xs = Cons(3, Cons(1, Cons(2, Nil())))
  let some-value on xs = length(tail(xs))
  some-value
}
